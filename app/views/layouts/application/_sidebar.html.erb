<div id="sidebar-wrapper" class="border-end bg-white">
  <div class="list-group list-group-flush">
    <%= link_to root_path, class: "list-group-item list-group-item-action p-3" do %>
      <%= render inline: Rails.root.join('app', 'assets', 'images', 'fluent_icons', 'regular', 'ic_fluent_home_24_regular.svg').read %> Home
    <% end %>
    <%= link_to admin_students_path, class: "list-group-item list-group-item-action p-3" do %>
      <%= render inline: Rails.root.join('app', 'assets', 'images', 'fluent_icons', 'regular', 'ic_fluent_people_24_regular.svg').read %> Students
