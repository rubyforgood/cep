    <%= link_to admin_tickets_path, class: "list-group-item list-group-item-action p-3" do %>
      <%= render inline: Rails.root.join('app', 'assets', 'images', 'fluent_icons', 'regular', 'ic_fluent_ticket_diagonal_24_regular.svg').read %> Tickets
