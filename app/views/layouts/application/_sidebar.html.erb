    <%= link_to admin_admin_users_path, class: "list-group-item list-group-item-action p-3" do %>
      <%= render inline: Rails.root.join('app', 'assets', 'images', 'fluent_icons', 'regular', 'ic_fluent_person_accounts_24_regular.svg').read %> Admins
